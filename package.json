{
  "private": true,
  "name": "los-danis-wedding",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "npm run clean && concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "clean": "rm -rf public/build && rm -rf build"
  },
  "dependencies": {
    "@remix-run/react": "^1.4.1",
    "@remix-run/serve": "^1.4.1",
    "@remix-run/server-runtime": "^1.4.1",
    "@remix-run/vercel": "^1.4.1",
    "@types/react-router-dom": "^5.3.3",
    "@vercel/node": "^1.14.0",
    "add-to-calendar-button": "^1.7.5",
    "i18next": "^21.6.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.2",
    "react-router-dom": "^6.3.0",
    "remix": "^1.4.1",
    "remix-i18next": "^3.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
